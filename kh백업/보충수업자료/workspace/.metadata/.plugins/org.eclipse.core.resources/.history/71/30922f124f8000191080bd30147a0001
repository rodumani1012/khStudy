package exception.ex;

public class Exception1_2 {

	public static void main(String[] args) {
		//난수저장
		int ran=0;
		
		//결과값 저장 
		double res=0.0;
		
		//0~9 난수 발생
		for(int i=0; i<10; i++) {
			ran= (int)(Math.random()*10);
			res = 10/ran;
		}
		
		
		
		//10/난수
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
